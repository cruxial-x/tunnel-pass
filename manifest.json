{
  "manifest_version": 3,
  "name": "Tunnel Pass",
  "version": "1.1",
  "description": "Bypass *.loca.lt login & redirect vite.local to localhost:5173",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "host_permissions": [
    "*://*.loca.lt/*",
    "*://vite/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}